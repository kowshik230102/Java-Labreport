import java.util.Vector;
class Main {
    public static void main(String[] args) {
        System.out.println("Enter the element of a vector of size 5 : ");
        Vector<Integer>V=new Vector<>(5);
        for(int i=0 ; i<5 ; i++)
        {
            V.add(2);
        }
        V.insertElementAt(6,1);
        V.insertElementAt(5,4);
        for(int it:V)
        {
            System.out.println(it+" ");
        }
    }
}
